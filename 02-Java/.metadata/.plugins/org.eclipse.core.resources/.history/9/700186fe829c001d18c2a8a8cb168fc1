package com.ssafy.day06.exception;

import java.util.Random;

class LottoNumberException extends RuntimeException{
	
}
class LottoService{
	private static final int MAX_NUM = 50;
	public int makeNumber() {
		Random r = new Random();
		int num = r.nextInt(45) + 1;
		if(num > 45) {
			throw
		}
		return num;
	}
}
