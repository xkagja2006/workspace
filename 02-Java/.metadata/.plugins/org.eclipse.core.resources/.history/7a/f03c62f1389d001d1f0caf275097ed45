package com.ssafy.day07.sort;

public class test04 {
	public static void main(String[] args) {
		Data d1 = new Data1("더글로리", "송혜교", 2022);
		Data d2 = new Data1("나의아저씨", "아이유", 2015);
		Data d3 = new Data1("주몽", "송일국", 2006);
		Data d4 = new Data1("미스터선샤인", "김태리", 2019);
		
		Data[] arr = { d1, d2, d3, d4 };
	}
}

class Data1 {
	String title;
	String name;
	int year;

	public Data1(String title, String name, int year) {
		super();
		this.title = title;
		this.name = name;
		this.year = year;
	}

	@Override
	public String toString() {
		return String.format("%s-%s-%d", title, name, year);
	}
}
